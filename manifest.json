{
   "manifest_version": 3,
   "name": "NarrowD",
   "version": "1.0.0",
   "description": "Narrow down your search results to the sites you want",
   "permissions": [
      "search",
      "storage"
   ],
   "host_permissions": [
      "*://*/*"
   ],
   "content_scripts": [
      {
         "matches": [
            "*://*/*"
         ],
         "run_at": "document_idle",
         "js": [
            "content-script.js"
         ],
         "css": [
            "content-style.css"
         ]
      }
   ],
   "background": {
      "service_worker": "background.js"
   },
   "action": {
      "default_title": "NarrowD",
      "default_popup": "popup.html",
      "default_icon": {
         "16": "unliked_16.png",
         "48": "unliked_48.png",
         "128": "unliked_128.png"
      }
   },
   "commands": {
      "_execute_action": {
         "suggested_key": {
            "default": "Ctrl+Shift+Space",
            "windows": "Ctrl+Shift+Space"
         }
      },
      "go_to_page_search_bar": {
         "suggested_key": {
            "default": "Ctrl+Space",
            "windows": "Ctrl+Space"
         },
         "description": "Go to a search bar in the current page"
      }
   },
   "icons": {
      "16": "unliked_16.png",
      "48": "unliked_48.png",
      "128": "unliked_128.png"
   }
}